   "source": [
    "import turtle\n",
    "\n",
    "t = turtle.Turtle()\n",
    "t.pensize(3)\n",
    "\n",
    "r = 200  # Bán kính lớn\n",
    "r_small = r / 2  # Bán kính nhỏ\n",
    "\n",
    "while True:\n",
    "    t.circle(r, 90)\n",
    "    t.circle(r_small, 90)  \n",
    "    \n",
    "    if t.heading() == 90:\n",
    "        break\n",
    "t.done()"
   ]
